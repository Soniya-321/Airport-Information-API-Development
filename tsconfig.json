{
    "compilerOptions": {
      "target": "ES2021",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strictPropertyInitialization": false,
      "moduleResolution": "node",
      "skipLibCheck": true,
      "types": ["node", "express"]
    }
  }
  